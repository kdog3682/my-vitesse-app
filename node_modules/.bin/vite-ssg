#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kdog3682/my-vitesse-app/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.22_vite@5.2.7_vue-router@4.3.0_vue@3.4.21/node_modules/vite-ssg/bin/node_modules:/home/kdog3682/my-vitesse-app/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.22_vite@5.2.7_vue-router@4.3.0_vue@3.4.21/node_modules/vite-ssg/node_modules:/home/kdog3682/my-vitesse-app/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.22_vite@5.2.7_vue-router@4.3.0_vue@3.4.21/node_modules:/home/kdog3682/my-vitesse-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kdog3682/my-vitesse-app/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.22_vite@5.2.7_vue-router@4.3.0_vue@3.4.21/node_modules/vite-ssg/bin/node_modules:/home/kdog3682/my-vitesse-app/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.22_vite@5.2.7_vue-router@4.3.0_vue@3.4.21/node_modules/vite-ssg/node_modules:/home/kdog3682/my-vitesse-app/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.22_vite@5.2.7_vue-router@4.3.0_vue@3.4.21/node_modules:/home/kdog3682/my-vitesse-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
fi
